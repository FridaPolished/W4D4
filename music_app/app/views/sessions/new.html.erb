<section class="page">
<div class="centered-box">
<h1> Sign in </h1>

<form action="<%= users_url %>" method="get">#?
<input
    type="hidden"
    name="authenticity_token"
    value='<%=form_authenticity_token%>'>

<label for="email">Email </label>
    <input type="text"
        name= "user[email]"
        id= "user_email">
<br>
<label for="password">Password </label>
    <input type="password"
        name= "user[password]"
        id= "user_password">
<br>
    <input type="submit">
    </form>
 </div>
</section>


